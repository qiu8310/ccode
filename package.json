{
  "name": "ccode",
  "version": "0.0.0",
  "description": "unicode character information",
  "main": "index.js",
  "bin": {
    "ccode": "scripts/bin/ccode.js"
  },
  "directories": {
    "doc": "docs"
  },
  "files": [
    "scripts/",
    "res/",
    "data/*.json",
    "README.md"
  ],
  "dependencies": {
    "async": "^1.4.2",
    "es6-shim": "^0.33.6",
    "fs-extra": "^0.24.0",
    "request": "^2.64.0",
    "tty-size": "^1.0.0",
    "tty-text": "^1.1.0",
    "tty-wrap": "0.1.1",
    "yargs-cn": "^4.0.1"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "mocha": "^2.3.3",
    "should": "^7.1.0"
  },
  "scripts": {
    "build": "rm -rf scripts && babel src -d scripts",
    "ling": "check-style --jsx src/*.jsx",
    "test": "mocha --compilers js:babel/register test/test-*",
    "cover": "rm -rf ./coverage && istanbul cover ./node_modules/mocha/bin/_mocha -- -R spec --compilers js:babel/register test/test-*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qiu8310/ccode.git"
